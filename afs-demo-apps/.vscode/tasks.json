    "version": "2.0.0",
    "tasks": [
        {
            "type": "npm",
            "script": "install",
            "path": "${workspaceFolder}/api",
        },
        {
            "type": "npm",
            "script": "test",
            "path": "${workspaceFolder}/api",
        },
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/id/id_service.csproj"
            ],
            "problemMatcher": "$msCompile"
        } 
    ]
}